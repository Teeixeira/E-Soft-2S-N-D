programa {
  funcao inicio() {
    real valor,valor2

    escreva("Informe um valor\n")
    leia(valor)
    escreva("Informe um segundo valor\n")
    leia(valor2)
    se(valor > valor2){
      escreva("O valor ",valor," é o maior dos dois")
    }senao
      {
        escreva("O valor ",valor2," é o maior dos dois")
      }


  }
}
